<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >


        <ImageButton
            android:id="@+id/staff_owner"
            android:layout_width="127dp"
            android:layout_height="120dp"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            android:background="@drawable/gk"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageButton
            android:id="@+id/staff_generalManager"
            android:layout_width="127dp"
            android:layout_height="120dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/lw"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.014"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/staff_owner" />


        <ImageButton
            android:id="@+id/staff_coach"
            android:layout_width="127dp"
            android:layout_height="120dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.014"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/staff_generalManager" />

        <ImageButton
            android:id="@+id/staff_assistantCoach"
            android:layout_width="127dp"
            android:layout_height="120dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.014"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/staff_coach" />

        <ImageButton
            android:id="@+id/staff_fitnessCoach"
            android:layout_width="127dp"
            android:layout_height="120dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.014"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/staff_assistantCoach" />

        <ImageButton
            android:id="@+id/staff_goalkeeperCoach"
            android:layout_width="127dp"
            android:layout_height="120dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.014"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/staff_fitnessCoach" />

        <ImageButton
            android:id="@+id/staff_physio"
            android:layout_width="127dp"
            android:layout_height="120dp"
            android:layout_marginTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.014"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/staff_goalkeeperCoach" />



        <TextView
            android:id="@+id/text_owner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="40dp"
            android:text="Bramkarz"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_owner"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_owner_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="Sebastian Łapacz"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_owner"
            app:layout_constraintTop_toBottomOf="@+id/text_owner" />

        <TextView
            android:id="@+id/text_generalManager"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="81dp"
            android:text="Lewy obrońca"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_generalManager"
            app:layout_constraintTop_toBottomOf="@+id/text_owner_name" />

        <TextView
            android:id="@+id/text_generalManager_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_generalManager"
            app:layout_constraintTop_toBottomOf="@+id/text_generalManager" />

        <TextView
            android:id="@+id/text_coach"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="81dp"
            android:text="Prawy obrońca"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_coach"
            app:layout_constraintTop_toBottomOf="@+id/text_generalManager_name" />

        <TextView
            android:id="@+id/text_coach_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_coach"
            app:layout_constraintTop_toBottomOf="@+id/text_coach" />

        <TextView
            android:id="@+id/text_assistantCoach"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="81dp"
            android:text="Środkowy obrońca"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_assistantCoach"
            app:layout_constraintTop_toBottomOf="@+id/text_coach_name" />

        <TextView
            android:id="@+id/text_assistantCoach_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_assistantCoach"
            app:layout_constraintTop_toBottomOf="@+id/text_assistantCoach" />

        <TextView
            android:id="@+id/text_fitnessCoach"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="81dp"
            android:text="Środkowy obrońca"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_fitnessCoach"
            app:layout_constraintTop_toBottomOf="@+id/text_assistantCoach_name" />

        <TextView
            android:id="@+id/text_fitnessCoach_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_fitnessCoach"
            app:layout_constraintTop_toBottomOf="@+id/text_fitnessCoach" />

        <TextView
            android:id="@+id/text_goalkeeperCoach"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="81dp"
            android:text="Defensywny pomocnik"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_goalkeeperCoach"
            app:layout_constraintTop_toBottomOf="@+id/text_fitnessCoach_name" />

        <TextView
            android:id="@+id/text_goalkeeperCoach_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_goalkeeperCoach"
            app:layout_constraintTop_toBottomOf="@+id/text_goalkeeperCoach" />

        <TextView
            android:id="@+id/text_physio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="81dp"
            android:text="Środkowy pomocnik"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_physio"
            app:layout_constraintTop_toBottomOf="@+id/text_goalkeeperCoach_name" />

        <TextView
            android:id="@+id/text_physio_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            android:layout_marginTop="10dp"
            android:text="TextView"
            android:textColor="@color/white"
            app:layout_constraintStart_toEndOf="@+id/staff_physio"
            app:layout_constraintTop_toBottomOf="@+id/text_physio" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>